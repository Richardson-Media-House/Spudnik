{
    "name": "spudnik",
    "description": "Spudnik is a free and open source Discord chat bot focused on guild management and adding interesting and useful functionality to your server. It was built on top of some of the most popular and widely used frameworks for extendibility, and written with organization and stability in mind. It also ships with ZERO weeb commands/references!",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "readme": "README.md",
    "engines": {
        "node": "^10.16.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Spudnik-Group/Spudnik.git"
    },
    "author": "Spudnik Group <comrades@spudnik.io> (https://spudnik.io)",
    "contributors": [
        "Nate Richardson <hello@naterichardson.com> (https://naterichardson.com)",
        "Matt Carroll <admin@develogix.com> (https://develogix.com)"
    ],
    "main": "dist/launch.js",
    "scripts": {
        "preinstall": "npm i -g node-gyp typescript@3.6.5 rimraf",
        "lint": "eslint --ext ts src/**",
        "clean:all": "npm run clean:node_modules && npm run clean:dist",
        "clean:node_modules": "rimraf ./node_modules",
        "clean:dist": "rimraf ./dist",
        "build": "tsc && copyfiles -u 2 \"./dist/src/*.js\" \"./dist/src/**/*.json\" \"./dist/src/**/*.js\" \"./dist/src/**/*.js.map\" ./dist && rimraf \"./dist/src\"",
        "test": "npm run lint",
        "prestart": "npm run clean:dist && npm run build",
        "start": "npm run start:prod",
        "start:dev": "node ./node_modules/nodemon/bin/nodemon.js",
        "start:prod": "node ./dist/launch.js"
    },
    "dependencies": {
        "@klasa/querybuilder": "^0.0.1",
        "@klasa/request-handler": "^0.0.3",
        "@klasa/utils": "^0.0.4",
        "axios": "^0.19.0",
        "bufferutil": "^4.0.1",
        "chalk": "^2.4.2",
        "d20": "^1.4.1",
        "discord.js": "discordjs/discord.js#fe7df70",
        "erlpack": "github:discordapp/erlpack",
        "klasa": "github:dirigeants/klasa#sgn",
        "leet": "^1.3.0",
        "markdown-escape": "1.0.2",
        "module-alias": "^2.2.2",
        "mongodb": "^3.3.4",
        "mw-dict": "^3.0.0",
        "remove-markdown": "0.3.0",
        "rollbar": "^2.5.1",
        "soap": "^0.29.0",
        "urban-dictionary": "^2.2.1",
        "url-unshort": "^5.0.0",
        "utf-8-validate": "^5.0.2",
        "wikijs": "^6.0.0",
        "zlib-sync": "^0.1.6",
        "zucc": "^0.1.2"
    },
    "devDependencies": {
        "@discordjs/collection": "0.1.5",
        "@types/chalk": "2.2.0",
        "@types/common-tags": "1.8.0",
        "@types/module-alias": "2.0.0",
        "@types/node": "10.17.18",
        "@typescript-eslint/eslint-plugin": "2.23.0",
        "@typescript-eslint/parser": "2.23.0",
        "common-tags": "1.8.0",
        "copyfiles": "^2.2.0",
        "eslint": "6.8.0",
        "eslint-config-klasa": "dirigeants/klasa-lint",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-node": "11.0.0",
        "eslint-plugin-prefer-arrow": "1.1.7",
        "eslint-plugin-unicorn": "16.1.1",
        "nodemon": "1.19.4",
        "typescript": "3.6.5"
    },
    "_moduleAliases": {
        "@utils": "./dist/lib/util",
        "@lib": "./dist/lib",
        "@root": "./dist"
    }
}
