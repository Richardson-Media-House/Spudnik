	"devDependencies": {
		"@types/mongoose": "^4.7.32",
		"@types/node": "^9.3.0",
		"xo": "^0.18.2"
	},
	"xo": {
		"rules": {
			"linebreak-style": "off",
			"no-prototype-builtins": "warn",
			"radix": "warn",
			"guard-for-in": "warn",
			"import/no-unresolved": "off",
			"camelcase": "off",
			"no-negated-condition": "warn",
			"object-curly-spacing": [
				"error",
				"always",
				{
					"arraysInObjects": false
				}
			]
		}
	},
	"scripts": {
		"launch": "node spudnik",
		"test": "xo"
	}
}
