{
	"name": "spudnik",
	"description": "Spudnik is a free and open source Discord chat bot focused on guild management and adding interesting and useful functionality to your server. It was built on top of some of the most popular and widely used frameworks for extendibility, and written with organization and stability in mind. It also ships with ZERO weeb commands/references!",
	"version": "0.9.0",
	"license": "Apache-2.0",
	"readme": "README.md",
	"engines": {
		"node": "^10.16.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Spudnik-Group/Spudnik.git"
	},
	"author": "Spudnik Group <comrades@spudnik.io> (https://spudnik.io)",
	"contributors": [
		"Nate Richardson <hello@naterichardson.com> (https://naterichardson.com)",
		"Matt Carroll <admin@develogix.com> (https://develogix.com)"
	],
	"main": "dist/launch.js",
	"scripts": {
		"preinstall": "npm i -g node-gyp typescript",
		"lint": "tslint -p ./tslint.json",
		"test": "npm run lint",
		"prestart": "rimraf ./dist && tsc",
		"start": "node ./node_modules/nodemon/bin/nodemon.js",
		"start:prod": "node ./dist/launch.js",
		"start:docs": "cd ./docs && bundle exec jekyll serve"
	},
	"dependencies": {
		"@discordjs/uws": "^11.149.1",
		"@scoutsdk/server-sdk": "^0.7.0",
		"chalk": "^2.4.2",
		"d20": "^1.4.1",
		"date-fns": "^1.30.1",
		"discord.js": "discordjs/discord.js#04fa56d",
		"discord.js-commando": "discordjs/Commando#d07be22",
		"erlpack": "github:discordapp/erlpack",
		"leet": "^1.3.0",
		"mongoose": "^5.4.21",
		"mw-dict": "^3.0.0",
		"node-superfetch": "^0.1.9",
		"remove-markdown": "0.3.0",
		"request": "^2.88.0",
		"request-promise": "^4.2.4",
		"rollbar": "^2.5.1",
		"soap": "^0.27.0",
		"urban-dictionary": "^2.2.1",
		"url-unshort": "^5.0.0",
		"wikijs": "^4.11.1",
		"zucc": "^0.1.2"
	},
	"devDependencies": {
		"@types/chalk": "2.2.0",
		"@types/common-tags": "1.8.0",
		"@types/date-fns": "2.6.0",
		"@types/mongoose": "5.5.8",
		"@types/node": "10.14.6",
		"@types/request-promise": "4.1.44",
		"nodemon": "1.19.0",
		"rimraf": "2.6.3",
		"tslint": "5.16.0",
		"tslint-jsdoc-rules": "0.2.0",
		"typescript": "3.4.5"
	}
}
